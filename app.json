{
  "expo": {
    "name": "Post Blog",
    "slug": "rn-posts",
    "privacy": "public",
    "sdkVersion": "39.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rn-icon.png",
    "splash": {
      "image": "./assets/rn-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#242832"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dmitpro.post-blog-rn",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.dmitpro.post_blog_rn",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}
